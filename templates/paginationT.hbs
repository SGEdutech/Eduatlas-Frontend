{{#if isPrevZero}} {{else}}
<li class="page-item">
    <a class="page-link prev-page" tabindex="-1">Previous</a>
</li>
<li class="page-item">
    <a class="page-link prev-page">{{pageM1}}</a>
</li>
{{/if}}
<li class="page-item active">
    <a class="page-link" style="background-color: #00bcd4;border-color: #00bcd4">{{page}}<span class="sr-only">(current)</span></a>
</li>
<li class="page-item">
    <a class="page-link next-page">{{pageP1}}</a>
</li>
<li class="page-item">
    <a class="page-link next-page">Next</a>
</li>